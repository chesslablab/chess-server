{
  "name": "chesslablab/chess-server",
  "description": "Asynchronous PHP chess server.",
  "authors": [
    {
      "name": "ChesslaBlab",
      "email": "info@chesslablab.org",
      "homepage": "https://github.com/chesslablab"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/chesslablab/php-chess.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "chesslablab/php-chess": "^1.4",
    "cboden/ratchet": "^0.4",
    "firebase/php-jwt": "^6.8",
    "vlucas/phpdotenv": "^5.3",
    "monolog/monolog": "^2.3",
    "react/async": "^4.1.0",
    "workerman/workerman": "^4.1",
    "spomky-labs/otphp": "^11.3",
    "spatie/async": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "ChessServer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChessServer\\Tests\\": "tests"
    }
  },
  "scripts": {
    "tests": "phpunit"
  }
}
